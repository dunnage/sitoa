	<!--  Vehicle  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Vehicle">
		<xsd:annotation>
			<xsd:documentation>Defines a vehicle specified for the delivery of the product.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="VehiclePreposition" type="SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples: Use, With, Dilute, Dissolve, Upon, Administer, Mix, Swish, Swish and swallow</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VehicleValue" type="SigValueOrRange"/>
			<xsd:element name="Vehicle" type="SigCode">
				<xsd:annotation>
					<xsd:documentation>Defines a vehicle specified for the delivery of the product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="VehicleClarifyingFreeText" type="an1..255" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to add clarity to the vehicle for elements that cannot be codified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
