			<xsd:element name="Product" type="ProductCF"/>
			<xsd:element name="Quantity" type="Quantity"/>
			<xsd:element name="DaysSupply" type="n1..3">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WrittenDate" type="DateType"/>
			<xsd:element name="LastFillDate" type="DateType"/>
			<xsd:element name="Substitutions" type="SubstitutionType">
				<xsd:annotation>
					<xsd:documentation>Code indicating the prescriber's instructions regarding generic or interchangeable biosimilar substitution were followed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationNumber" type="an1..35" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the payer-assigned authorization number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..210" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sig" type="Sig" minOccurs="0"/>
			<xsd:element name="PharmacyRequestedRefills" type="n1..2" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="DoNotFill" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RefillsRemaining" type="n1..5" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of refills remaining in the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPresciberOrderNumber" type="an1..35" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrderCaptureMethod" type="OrderCaptureMethod" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="OptionalPharmacy" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prescriber" type="HistoryPrescriberChoice" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="HistorySource" type="HistorySourceWithoutPayment" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ReasonForSubstitutionCodeUsed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = DispensedAsWritten, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitScript" type="SplitScriptIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Two prescriptions for the same product that are sent to two different entities for fulfillment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManufacturerName" type="an1..105">
				<xsd:annotation>
					<xsd:documentation>Name of the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="LotNumber" minOccurs="0" maxOccurs="9"/>
			<xsd:element name="NumberOfPackagesDispensed" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If it is not sent it is implied it is 1, otherwise send actual number that was dispensed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherMedicationDates" type="OtherMedicationDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedNoLaterThan" type="NeedNoLaterThan" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="ProcedureCodes" type="QualifiedCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used to transmit procedure codes, such as CDT and CPT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CFWarningLabel  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CFWarningLabel">
		<xsd:sequence>
			<xsd:element name="LabelPriority" type="n1..1"/>
			<xsd:element name="GraphicCode" type="an1..3"/>
			<xsd:element name="LabelCode" type="an1..6"/>
			<xsd:element name="LabelText" type="an1..100"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CFWarningLabelForRxFill  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CFWarningLabelForRxFill">
		<xsd:sequence>
			<xsd:element name="LabelPriority" type="n1..1"/>
			<xsd:element name="GraphicCode" type="an1..3" minOccurs="0"/>
			<xsd:element name="LabelCode" type="an1..6" minOccurs="0"/>
			<xsd:element name="LabelText" type="an1..100"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ClinicalInformationFormatSupported  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ClinicalInformationFormatSupported">
		<xsd:sequence>
			<xsd:element name="CCR" type="CCRVersions" minOccurs="0"/>
			<xsd:element name="CDA" type="CDATemplates" minOccurs="0"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CodedReference  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CodedReference">
		<xsd:sequence>
			<xsd:element name="CodeSystemVersion" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The version of the code system used.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Qualifier" type="CodedReferenceQualifier">
				<xsd:annotation>
					<xsd:documentation>Qualifier to identify the code system being used.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Code" type="Code">
				<xsd:annotation>
					<xsd:documentation>The code from the source.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="an">
				<xsd:annotation>
					<xsd:documentation>The textual representation of the CodedReferenceCode.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CodedReferenceAnswerChoiceDateType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CodedReferenceAnswerChoiceDateType">
		<xsd:choice>
			<xsd:element name="Date" type="DateType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SeeTransactionLevelAttachmentControlNumber" type="an">
				<xsd:annotation>
					<xsd:documentation>Indicates to see attachment at the transaction level instead of an attachment at a completed answer level.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnswerValue" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Coded reference value for the answer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Attachment" type="AttachmentWithControlNumber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CodedReferenceAnswerChoiceFreeTextType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CodedReferenceAnswerChoiceFreeTextType">
		<xsd:choice>
			<xsd:element name="SeeTransactionLevelAttachmentControlNumber" type="an">
				<xsd:annotation>
					<xsd:documentation>Indicates to see attachment at the transaction level instead of an attachment at a completed answer level.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnswerValue" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Coded reference value for the answer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Attachment" type="AttachmentWithControlNumber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CodedReferenceAnswerChoiceNumericType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CodedReferenceAnswerChoiceNumericType">
		<xsd:choice>
			<xsd:element name="NumericAnswer" type="sn1..18"/>
			<xsd:element name="SeeTransactionLevelAttachmentControlNumber" type="an">
				<xsd:annotation>
					<xsd:documentation>Indicates to see attachment at the transaction level instead of an attachment at a completed answer level.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnswerValue" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Coded reference value for the answer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Attachment" type="AttachmentWithControlNumber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CodedReferenceAnswerChoiceSelectType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CodedReferenceAnswerChoiceSelectType">
		<xsd:choice>
			<xsd:element name="Choice" type="AnswerChoiceForPARequest"/>
			<xsd:element name="SeeTransactionLevelAttachmentControlNumber" type="an">
				<xsd:annotation>
					<xsd:documentation>Indicates to see attachment at the transaction level instead of an attachment at a completed answer level.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnswerValue" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Coded reference value for the answer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Attachment" type="AttachmentWithControlNumber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CompletedAnswerChoiceForDateType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CompletedAnswerChoiceForDateType">
		<xsd:choice>
			<xsd:element name="SubmitterProvidedAnswer" type="DateType">
				<xsd:annotation>
					<xsd:documentation>Free text answer provided by submitter.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReferenceAnswer" type="CodedReferenceAnswerChoiceDateType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CompletedAnswerChoiceForFreeTextType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CompletedAnswerChoiceForFreeTextType">
		<xsd:choice>
			<xsd:element name="SubmitterProvidedAnswer" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Free text answer provided by submitter.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReferenceAnswer" type="CodedReferenceAnswerChoiceFreeTextType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CompletedAnswerChoiceForNumericType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CompletedAnswerChoiceForNumericType">
		<xsd:choice>
			<xsd:element name="SubmitterProvidedNumericAnswer" type="sn1..18">
				<xsd:annotation>
					<xsd:documentation>Numeric answer provided by submitter.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReferenceAnswer" type="CodedReferenceAnswerChoiceNumericType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CompletedAnswerChoiceForSelectType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CompletedAnswerChoiceForSelectType">
		<xsd:choice>
			<xsd:element name="SubmitterProvidedAnswer" type="AnswerChoiceForPARequest">
				<xsd:annotation>
					<xsd:documentation>Free text answer provided by submitter.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReferenceAnswer" type="CodedReferenceAnswerChoiceSelectType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateGroupWithEffectiveExpiredDate  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateGroupWithEffectiveExpiredDate">
		<xsd:sequence>
			<xsd:element name="EffectiveDate" type="DateType"/>
			<xsd:element name="ExpirationDate" type="DateType"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateTypeForRequest  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateTypeForRequest">
		<xsd:sequence>
			<xsd:element name="Date" type="PADateTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Answer" type="CompletedAnswerChoiceForDateType" minOccurs="0"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateValueSimpleType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateValueSimpleType">
		<xsd:sequence>
			<xsd:element name="ComparisonOperator" type="ComparisonOperator">
				<xsd:annotation>
					<xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined boundary.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ComparisonDate" type="DateType">
				<xsd:annotation>
					<xsd:documentation>Value for the boundary of the comparison.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextQuestionID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateValueType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateValueType">
		<xsd:sequence>
			<xsd:element name="LowerBoundComparisonDate" type="DateType">
				<xsd:annotation>
					<xsd:documentation>Date for the lower boundary of the comparison.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LowerBoundComparisonOperator" type="LowerBoundComparisonOperator">
				<xsd:annotation>
					<xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined lower boundary</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UpperBoundComparisonDate" type="DateType">
				<xsd:annotation>
					<xsd:documentation>Date for the upper boundary of the comparison.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UpperBoundComparisonOperator" type="UpperBoundComparisonOperator">
				<xsd:annotation>
					<xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined upper boundary.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextQuestionID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateTypeComparision  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateTypeComparision">
		<xsd:choice>
			<xsd:element name="SingleComparison" type="DateValueSimpleType"/>
			<xsd:element name="RangeComparison" type="DateValueType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Diagnosis  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Diagnosis">
		<xsd:sequence>
			<xsd:element name="ClinicalInformationQualifier" type="ClinicalInformationQualifier">
				<xsd:annotation>
					<xsd:documentation>Qualifies how the diagnosis was obtained.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Qualifier" type="DiagnosisCodeQualifierCode">
				<xsd:annotation>
					<xsd:documentation>Qualifies the code list used for the diagnosis.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Value" type="an">
				<xsd:annotation>
					<xsd:documentation>Code for the diagnosis.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="an">
				<xsd:annotation>
					<xsd:documentation>Textual representation of the diagnosis.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffectiveDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The beginning date or date and time. When this particular diagnosis was determined for the patient and when it was no longer active – if known.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpirationDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When this particular diagnosis was determined for the patient and when it was no longer active – if known.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateOfLastOfficeVisit" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date of the last office visit for this diagnosis.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DigitalSignature  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DigitalSignature">
		<xsd:annotation>
			<xsd:documentation>The DigitalSignature element is used for controlled substances for MedicationPrescribed information in NewRx, RxChangeResponse and RxRenewalResponse transactions as these are “fillable” prescription transactions per the regulation. Transactions that are not fillable are not required to be signed.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:sequence maxOccurs="2">
					<xsd:annotation>
						<xsd:documentation>Repeats twice to allow transitioning to a new hashing method.</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="DigestMethod" type="DigestMethod">
						<xsd:annotation>
							<xsd:documentation>Defines the Hashing method, eg. SHA-1 or SHA-256 to be used.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DigestValue" type="an">
						<xsd:annotation>
							<xsd:documentation>This field is used to confirm that all fields have been included in the digital signature. Apply a SHA1 Hash and base64 encode the result.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SignatureValue" type="an">
						<xsd:annotation>
							<xsd:documentation>DigestValue that has been signed with a Private Key, base64 encode the results.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="X509Data" type="an">
						<xsd:annotation>
							<xsd:documentation>Base64 encoded raw bytes of the X509 certificate (which contains the Public Key). Used by the receiver to validate the digital signature.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:element name="DigitalSignatureIndicator" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>True/False indicating the prescription has been digitally signed.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="Version" type="an" use="required">
			<xsd:annotation>
				<xsd:documentation>The digital signature version used.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Dispenser  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Dispenser">
		<xsd:choice>
			<xsd:element name="Pharmacy" type="MandatoryAddressPharmacy"/>
			<xsd:element name="Facility" type="Facility"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Dosage  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Dosage">
		<xsd:sequence>
			<xsd:element name="DoseValue" type="DoseValueOrRange"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DoseAdministration  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DoseAdministration">
		<xsd:annotation>
			<xsd:documentation>When DoseAdministration is present, it SHALL contain at least one of each: Delivery Method, Dose, DoseUnitOfMeasure, and Route. When appropriate for medication administration, Site and/or Vehicle shall be included. Elements may repeat for multiple instructions or ranges etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:element name="DoseDeliveryMethod" type="SigCode">
					<xsd:annotation>
						<xsd:documentation>Examples: Take, Apply, Swish, Swallow, Inject, Insert, Chew, Use, Give, Sprinkle, Mix, Dissolve…</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DoseDeliveryMethodModifier" type="SigCode" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Supports the use of modifiers such as Repeatedly, Deeply, Completely, Thoroughly, Sparingly, Constantly, Only, Gently, Vigorously, Lightly, Rapidly, Liberally, Slowly…</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:choice minOccurs="0">
				<xsd:element name="Dosage" type="Dosage" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Codifies the amount or numerical quantity and unit of the medication to be administered to the patient</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DoseCalculation" type="DoseCalculation" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>This field may often be used in conjunction with the Rate fields within the Timing Segment and/or the Vehicle Segment. Primary use will be in non-retail settings, i.e. hospital, acute care, long term care, home care.  An example using a dose calculation: Cyclophosphamide:  600mg/meter squared in 250ml Normal Saline over 1-2 hours.  Repeat every 2 weeks.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DoseAmount" type="SigCode" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Examples: Thin ribbon, Thick ribbon, Small amount, Sufficient amount, Thin film, Thick film, Dollop, Liberal amount, Pea-sized amount</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DoseClarifyingFreeText" type="an1..255" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Used to add clarity to the dose for elements that cannot be codified.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="Vehicle" type="Vehicle" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Defines a vehicle specified for the delivery of the product. Examples: Fruit juice, Applesauce, Milk </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RouteOfAdministration" type="SigCode">
				<xsd:annotation>
					<xsd:documentation>Defines the route of administration. 
Examples: By mouth, In ear, Topical route, Inhalation route, Ocular route, Intramuscular route, Intravenous route, Rectal route</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RouteOfAdministrationClarifyingFreeText" type="an1..255" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to add clarity to the administration timing for elements that cannot be codified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence minOccurs="0">
				<xsd:element name="SiteOfAdministration" type="SigCode">
					<xsd:annotation>
						<xsd:documentation>Defines the site of administration.
Examples: Affected area, Left Eye, Right Eye, Both Eyes, Left Nostril, Right Nostril, Both Nostrils</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="SiteOfAdministrationClarifyingFreeText" type="an1..255" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Used to add clarity to the site of administration for elements that cannot be codified.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DoseCalculation  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DoseCalculation">
		<xsd:annotation>
			<xsd:documentation>Used to calculate the dosage based on patient weight or surface area.  Dosing Basis and Unit Of Measure are required. Patient information and the results of the calculation may optionally be included; the sender may allow the recipient to use the included information to calculate the final dosage. Example: "Calculated dose based on 5 milligram per kilogram and a patient weight of 70 kg, for a final dose of 350 milligram."</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DoseBasisValue" type="DoseValueOrRange"/>
			<xsd:element name="BodyMetricQualifier" type="BodyMetricQualifier">
				<xsd:annotation>
					<xsd:documentation>Qualifier to identify the body metric being used (either weight or surface area).
1 = Kilogram
2 = Meter Squared</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BodyMetricValue" type="n1..18" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Expresses the value of the body metric. Patients Weight or Body Surface area of the medication needed.
Example: 70</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CalculatedDoseValue" type="DoseValueOrRange" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The Calculated Dose Numeric can be populated with either the information as specified by the prescriber or by performing the calculation using the Dosing Basis and Body Metric elements. For example, if the Dosing Basis Numeric Value is 600 and the Dosing Basis Numeric Value Text is milligrams per kilogram, the prescriber’s system may send the patient’s weight in Body Metric Value and then populate the result of the calculation in this field. Alternatively, the prescriber’s system may send the patient’s weight in Body Metric Value and allow the pharmacist to use that information to calculate/validate the dose.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DoseValueOrRange  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DoseValueOrRange">
		<xsd:sequence>
			<xsd:element name="Value1" type="n" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Numeric value</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence minOccurs="0">
				<xsd:element name="RangeModifier" type="VariableModifier">
					<xsd:annotation>
						<xsd:documentation>Optional Modifier to support ranges (AND, OR, TO)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Value2" type="n">
					<xsd:annotation>
						<xsd:documentation>Optional 2nd value in a Range</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:element name="DoseUnitOfMeasure" type="DoseUnitOfMeasureCode" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Unit of Measure for the Dose..</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DrugAdministrationMedication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DrugAdministrationMedication">
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="an1..105">
				<xsd:annotation>
					<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If Compound, this field must contain the prescriber’s preferred name of the compound.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Product" type="ProductCompoundLotNotUsed" minOccurs="0"/>
			<xsd:element name="Quantity" type="Quantity"/>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WrittenDate" type="DateType"/>
			<xsd:element name="LastFillDate" type="DateType" minOccurs="0"/>
			<xsd:element name="Substitutions" type="SubstitutionType">
				<xsd:annotation>
					<xsd:documentation>Code indicating the prescriber's instructions regarding generic or interchangeable biosimilar substitution were followed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PriorAuthorizationNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the payer-assigned authorization number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..210" minOccurs="0"/>
			<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.  When this composite is used, ServiceReasonCode is mandatory. When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.  When the ServiceReasonCode is sent from the pharmacist to the prescriber, The ServiceResultCode is conditional.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sig" type="Sig"/>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ReasonForSubstitutionCodeUsed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RxFillIndicator" type="RxFillIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the status types the sender would like to receive for this patient/product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Service" type="ServiceStructure" minOccurs="0"/>
			<xsd:element name="IVAdministration" type="IVAdministrationType" minOccurs="0"/>
			<xsd:element name="TreatmentIndicator" type="TreatmentIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProphylacticOrEpisodic" type="ProphylacticOrEpisodic" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrentTreatmentCycle" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfCyclesPlanned" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfPackagesToBeDispensed" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If it is not sent it is implied it is 1, otherwise send desired number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherMedicationDates" type="OtherMedicationDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FacilitySpecificHoursOfAdministrationTiming" type="FacilitySpecificHoursOfAdministrationTiming" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Provides information to the dispensing pharmacy which clarifies/justifies the product selection, dispensing, or instructs the dispensing pharmacy to take specific action per request of the provider.   It codifies items commonly found in the element Note; and is specific to the patient and the medication/product being prescribed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProcedureCodes" type="QualifiedCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used to transmit procedure codes, such as CDT and CPT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Duration  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Duration">
		<xsd:sequence>
			<xsd:element name="DurationNumericValue" type="n">
				<xsd:annotation>
					<xsd:documentation>The numeric duration units.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DurationText" type="SigCode"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DurationTriggerTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DurationTriggerTiming">
		<xsd:sequence>
			<xsd:element name="Trigger" type="SigCode"/>
			<xsd:element name="DurationClarifyingFreeText" type="an1..255" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to add clarity to the duration for elements that cannot be codified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Facility  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Facility">
		<xsd:annotation>
			<xsd:documentation>The Facility composite may be included to describe the care facility or other location at which the patient resides.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="FacilityID">
				<xsd:annotation>
					<xsd:documentation>Identification of the facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FacilityName" type="an1..70" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name identifying the location of the service rendered.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FacilitySpecificHoursOfAdministrationTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FacilitySpecificHoursOfAdministrationTiming">
		<xsd:annotation>
			<xsd:documentation>sed to communicate facility specific standardized administration times when specific times or days are not entered in the prescribed directions.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="HoursOfAdministrationValue" type="n1..5" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>The time for the administration or the product in military time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HoursOfAdministration" type="SigCode">
				<xsd:annotation>
					<xsd:documentation>The code from the source.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FinancialGuarantorType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FinancialGuarantorType">
		<xsd:sequence>
			<xsd:element name="Names" type="NameComposite" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BusinessName" type="an1..70" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>When pharmacist is not associated with a pharmacy (see Pharmacy element) and a separate business name is needed.
			</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FinancialGuarantorRelationship" type="FinancialGuarantorRelationship">
				<xsd:annotation>
					<xsd:documentation>Code indicating the relationship between the financial guarantor and the patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FollowUpPrescriber  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FollowUpPrescriber">
		<xsd:annotation>
			<xsd:documentation>Identifies the prescriber to whom the pharmacy should direct subsequent communication for a given prescription and/or given patient.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="FollowUpPrescriberID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Specialty" type="an1..10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specialty of prescriber. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Names" type="NameComposite">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FollowUpPrescriberType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FollowUpPrescriberType">
		<xsd:choice>
			<xsd:element name="NonVeterinarian" type="FollowUpPrescriber" minOccurs="0"/>
			<xsd:element name="Veterinarian" type="VetSupervisorMirrorSupervisorOptional" minOccurs="0"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FreeTextTypeBasic  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FreeTextTypeBasic">
		<xsd:sequence>
			<xsd:element name="IsFreeText" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>A value of "True" to indicate the answer to the question is free text. A value of "False" should not be used. Use other question type if not free text.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FrequencyTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FrequencyTiming">
		<xsd:sequence>
			<xsd:element name="FrequencyNumericValue" type="SigValueOrRange"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HistoryDispensedMedication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HistoryDispensedMedication">
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="an1..105">
				<xsd:annotation>
					<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If Compound, this field must contain the prescriber’s preferred name of the compound.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Product" type="ProductMandatory" minOccurs="0"/>
			<xsd:element name="Quantity" type="Quantity"/>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WrittenDate" type="DateType" minOccurs="0"/>
			<xsd:element name="LastFillDate" type="DateType"/>
			<xsd:element name="Substitutions" type="SubstitutionType">
				<xsd:annotation>
					<xsd:documentation>Code indicating the prescriber's instructions regarding generic or interchangeable biosimilar substitution were followed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The refill quantity of the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PriorAuthorizationNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the payer-assigned authorization number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..210" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sig" type="Sig" minOccurs="0"/>
			<xsd:element name="PharmacyRequestedRefills" type="n1..2" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="DoNotFill" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RefillsRemaining" type="n1..5" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of refills remaining in the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPrescriberOrderNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrderCaptureMethod" type="OrderCaptureMethod" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="OptionalPharmacy" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prescriber" type="HistoryPrescriberChoice" minOccurs="0"/>
			<xsd:element name="HistorySource" type="HistorySourceWithPayment" minOccurs="0"/>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ReasonForSubstitutionCodeUsed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitScript" type="SplitScriptIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Two prescriptions for the same product that are sent to two different entities for fulfillment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManufacturerName" type="an1..105" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="LotNumber" minOccurs="0" maxOccurs="9"/>
			<xsd:element name="RxFillIndicator" type="RxFillIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the status types the sender would like to receive for this patient/product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Patient" type="Patient" minOccurs="0"/>
			<xsd:element name="NumberOfPackagesDispensed" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If it is not sent it is implied it is 1, otherwise send actual number that was dispensed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherMedicationDates" type="OtherMedicationDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedNoLaterThan" type="NeedNoLaterThan" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="ProcedureCodes" type="QualifiedCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used to transmit procedure codes, such as CDT and CPT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HistoryPrescribedMedication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HistoryPrescribedMedication">
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="an1..105">
				<xsd:annotation>
					<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If Compound, this field must contain the prescriber’s preferred name of the compound.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Product" type="ProductMandatory" minOccurs="0"/>
			<xsd:element name="Quantity" type="Quantity"/>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WrittenDate" type="DateType"/>
			<xsd:element name="LastFillDate" type="DateType" minOccurs="0"/>
			<xsd:element name="Substitutions" type="SubstitutionType">
				<xsd:annotation>
					<xsd:documentation>Code indicating the prescriber's instructions regarding generic or interchangeable biosimilar substitution were followed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="n1..2">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PriorAuthorizationNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the payer-assigned authorization number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..210" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sig" type="Sig" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PharmacyRequestedRefills" type="n1..2" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="DoNotFill" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RefillsRemaining" type="n1..5" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The number of refills remaining in the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPrescriberOrderNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrderCaptureMethod" type="OrderCaptureMethod" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="OptionalPharmacy" minOccurs="0"/>
			<xsd:element name="Prescriber" type="HistoryPrescriberChoice" minOccurs="0"/>
			<xsd:element name="HistorySource" type="HistorySourceWithoutPayment" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ReasonForSubstitutionCodeUsed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitScript" type="SplitScriptIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Two prescriptions for the same product that are sent to two different entities for fulfillment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManufacturerName" type="an1..105" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="LotNumber" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="RxFillIndicator" type="RxFillIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the status types the sender would like to receive for this patient/product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Patient" type="Patient" minOccurs="0"/>
			<xsd:element name="Wound" type="WoundStructure" minOccurs="0" maxOccurs="9"/>
			<xsd:element name="NumberOfPackagesToBeDispensed" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If it is not sent it is implied it is 1, otherwise send desired number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherMedicationDates" type="OtherMedicationDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedNoLaterThan" type="NeedNoLaterThan" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="ProviderExplicitAuthorizationToAdminister" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates prescribers explicit authorization for a pharmacist to administer a product, vaccine, or device to a patient. To be sent when explicit permissive authority to administer a medication, device or vaccine is required by statute or regulation.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProcedureCodes" type="QualifiedCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used to transmit procedure codes, such as CDT and CPT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TypeOfDose" type="TypeOfDoseCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code representing the type of dose being requested.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HistoryPrescriber  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HistoryPrescriber">
		<xsd:sequence>
			<xsd:element name="Identification" type="NonVeterinarianID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>One occurrence is the prescriber number. One occurrence may be used to identify the clinic. At least one occurrence must contain the Prescriber individual (not organizational) NPI Number. DEANumber is sent when applicable.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Specialty" type="an1..10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specialty of prescriber. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Names" type="NameComposite" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType" minOccurs="0"/>
			<xsd:element name="PrescriberAgent" type="NameComposite" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0"/>
			<xsd:element name="PrescriberPlaceOfService" type="n2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the place where professional services are rendered resulting in the prescription/order. Required when place of service for the encounter is documented in the patient record.  Otherwise, required when needed to comply with state or federal programs. The CMS Place of Service code set as the ECL values https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set.html</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HistoryPrescriberChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HistoryPrescriberChoice">
		<xsd:choice>
			<xsd:element name="NonVeterinarian" type="HistoryPrescriber"/>
			<xsd:element name="Veterinarian" type="VetPrescriberMirrorHistoryPrescriber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  IVAdministrationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="IVAdministrationType">
		<xsd:sequence>
			<xsd:element name="NumberOfLumens" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the number of lumens in the IV access line that will be used for administering the medication.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DiluentAmount" type="QuantityWithoutSecondQuantity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the amount of diluent (which will have a separate order) to be used for administration. Concepts of the intended or actual diluent amount unit of measure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationGauge" type="n" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the size (gauge) of the needle or catheter being used for medication administration, i.e. 14, 18, 20.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationBrand" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the specific Brand of the PICC line used for central IV access.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationLength" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the size (length) of the PICC line used for central IV access.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationPump" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="IVAccessType" type="SNOMEDCode"/>
			<xsd:element name="IVAccessDeviceType" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IVAccessDeviceTypeDescription" type="an" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the specific device used for central IV access, if applicable i.e. implanted port, PICC line, tunneled line, non-tunneled line.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IVAccessDeviceType" type="SNOMEDCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The code representing the IVAccessDeviceType. It is the intended specific device used for central IV access.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="IVAccessCatheterTip" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IVAccessCatheterTipDescription" type="an" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the specific structure of the catheter tip for the specific device used for central or midline IV access., if applicable i.e. valved, non-valved.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IVAccessCatheterTipType" type="SNOMEDCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The code representing the IVAccessCatheterTipType. It is the intended specific structure of the catheter tip for the specific device used for central or midline IV access.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="IVInfusion" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IVInfusionDescription" type="an" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the intended type of IV infusion i.e. continuous, intermittent, TOTAL perenteral nutrition (TPN), PARTIAL perenteral nutrition (PPN), maintenance.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IVInfusionType" type="SNOMEDCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The code representing the IVInfusionTypeType. It is the intended type of IV infusion.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Indication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Indication">
		<xsd:sequence>
			<xsd:element name="IndicationPrecursor" type="SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as As needed for, As directed for, When, If, …</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Indication" type="SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as Blood Pressure, Finger Stick Blood Glucose</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IndicationRelation" type="IndicationRelationValue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as equal to, greater than, less than, between</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IndicationValue" type="SigValueOrRange" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value or Range of values for the Indication.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Instruction  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Instruction">
		<xsd:choice>
			<xsd:sequence>
				<xsd:element name="InstructionIndicator" type="InstructionIndicator">
					<xsd:annotation>
						<xsd:documentation>Indicates the action to be taken on the Instruction fields. When structured Sig is 'Use as directed during medical encounter.' Skip DoseAdministration. TimingAndDuration is Situational.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence minOccurs="0" maxOccurs="49">
					<xsd:element name="MultipleTimingModifier" type="MultipleTimingModifier" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>May be used to linked to any of the timing methods – Administration Timing and/or Frequency and/or Interval.  For example,” take each morning, before bed and every four hours as needed.”</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TimingAndDuration" type="TimingAndDuration"/>
				</xsd:sequence>
				<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:choice>
					<xsd:element name="AdministrationIndicator" type="AdministrationIndicator">
						<xsd:annotation>
							<xsd:documentation>Indicates the action to be taken on the Administration fields. When structured Sig is 'Use as directed per manufacturer instructions.' 
Skip DoseAdministration and TimingAndDuration.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:sequence>
						<xsd:element name="DoseAdministration" type="DoseAdministration"/>
						<xsd:element name="TimingAndDuration" type="TimingAndDuration">
							<xsd:annotation>
								<xsd:documentation>This element is used to provide instruction about the timing of the Sig -– when/how often/at what rate/for how long -– the medication is to be taken. Specific time fields must be Consistent with ISO format.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence minOccurs="0" maxOccurs="49">
							<xsd:element name="MultipleTimingModifier" type="MultipleTimingModifier" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>May be used to linked to any of the timing methods – Administration Timing and/or Frequency and/or Interval.  For example,” take each morning, before bed and every four hours as needed.”</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="TimingAndDuration" type="TimingAndDuration"/>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:choice>
				<xsd:sequence minOccurs="0">
					<xsd:element name="IndicationForUse" type="Indication" maxOccurs="50">
						<xsd:annotation>
							<xsd:documentation>Defines the indication for use of the medication as meant to be conveyed to the patient. Note:  The indication for use of the medication as meant to be conveyed to the patient may differ from the clinical indication for use.  As an example, take for sleep may be conveyed to the patient; the clinical indication may be insomnia.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IndicationClarifyingFreeText" type="an1..255" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used to add clarity to the indication for elements that cannot be codified.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:element name="MaximumDoseRestriction" type="MaximumDoseRestriction" minOccurs="0" maxOccurs="50">
					<xsd:annotation>
						<xsd:documentation>Defines a maximum or dose limit as specified by the prescriber. The value entered may or may not match with the information provided by clinical decision support tools.  Professional judgment should be used when discrepancies occur.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  IntervalTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="IntervalTiming">
		<xsd:sequence>
			<xsd:element name="IntervalValue" type="SigValueOrRange"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryAddressPharmacy  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryAddressPharmacy">
		<xsd:complexContent>
			<xsd:restriction base="MandatoryPharmacy">
				<xsd:sequence>
					<xsd:element name="Identification" type="PharmacyID"/>
					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specialty of pharmacy. Uses NUCC taxonomy list.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacist" type="Pharmacist" minOccurs="0"/>
					<xsd:element name="BusinessName" type="an1..70"/>
					<xsd:element name="Address" type="MandatoryAddressType"/>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType"/>
					<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryPharmacy  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryPharmacy">
		<xsd:complexContent>
			<xsd:restriction base="Pharmacy">
				<xsd:sequence>
					<xsd:element name="Identification" type="PharmacyID">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specialty of pharmacy. Uses NUCC taxonomy list.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacist" type="Pharmacist" minOccurs="0"/>
					<xsd:element name="BusinessName" type="an1..70">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="AddressType" minOccurs="0"/>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType"/>
					<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryPrescriber  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryPrescriber">
		<xsd:complexContent>
			<xsd:restriction base="Prescriber">
				<xsd:sequence>
					<xsd:element name="Identification" type="NonVeterinarianID">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specialty of prescriber. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Names" type="NameComposite">
						<xsd:annotation>
							<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="MandatoryAddressType"/>
					<xsd:element name="PrescriberAgent" type="NameComposite" minOccurs="0"/>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType"/>
					<xsd:element name="PrescriberPlaceOfService" type="n2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the place where professional services are rendered resulting in the prescription/order. Required when place of service for the encounter is documented in the patient record.  Otherwise, required when needed to comply with state or federal programs. The CMS Place of Service code set as the ECL values https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set.html</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryPrescriberChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryPrescriberChoice">
		<xsd:choice>
			<xsd:element name="NonVeterinarian" type="MandatoryPrescriber"/>
			<xsd:element name="Veterinarian" type="VetPrescriberMirrorMandatoryPrescriber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MaximumDoseRestriction  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MaximumDoseRestriction">
		<xsd:sequence>
			<xsd:element name="MaximumDoseRestrictionNumericValue" type="n"/>
			<xsd:choice>
				<xsd:element name="MaximumDoseRestrictionForm" type="DoseUnitOfMeasureCode"/>
				<xsd:element name="MaximumDoseRestrictionUnits" type="SigCode"/>
			</xsd:choice>
			<xsd:element name="MaximumDoseRestrictionDurationValue" type="n"/>
			<xsd:element name="MaximumDoseRestrictionDurationUnit" type="SigCode"/>
			<xsd:element name="MaximumDoseRestrictionClarifyingFreeText" type="an1..255" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to add clarity to the maximum dose restriction for elements that cannot be codified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementDuration  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementDuration">
		<xsd:sequence>
			<xsd:element name="MeasurementDurationNumericValue" type="n"/>
			<xsd:element name="MeasurementDurationText" type="SigCode"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementDurationTriggerTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementDurationTriggerTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementTrigger" type="SigCode"/>
			<xsd:element name="MeasurementDurationClarifyingFreeText" type="an1..255" minOccurs="0"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementFrequencyTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementFrequencyTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementFrequencyValue" type="SigValueOrRange"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementIntervalTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementIntervalTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementIntervalValue" type="SigValueOrRange"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementTimingValue" type="SigValueOrRange"/>
			<xsd:element name="MeasurementTimingModifier" type="SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as before, during, with, without, after, while, at., on, start, end.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MeasurementTimingEvent" type="SigCode"/>
			<xsd:element name="MeasurementTimingClarifyingFreeText" type="an1..255" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to add clarity to the titration measurement timing for elements that cannot be codified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Medication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Medication">
		<xsd:annotation>
			<xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="an1..105">
				<xsd:annotation>
					<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If Compound, this field must contain the prescriber’s preferred name of the compound.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Product" type="ProductCompoundLotNotUsed" minOccurs="0"/>
			<xsd:element name="Quantity" type="QuantityAbstract" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="WrittenDate" type="DateType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="LastFillDate" type="DateType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DeliveredOnDate" type="DateType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Substitutions" type="SubstitutionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code indicating the prescriber's instructions regarding generic or interchangeable biosimilar substitution were followed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The refill quantity of the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PriorAuthorizationNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the payer-assigned authorization number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..210" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sig" type="Sig" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PharmacyRequestedRefills" type="n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Number of refills the pharmacy is requesting.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="DoNotFill" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RefillsRemaining" type="n1..5" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The number of refills remaining in the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPrescriberOrderNumber" type="an1..35" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="OrderCaptureMethod" type="OrderCaptureMethod" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="OptionalPharmacy" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Prescriber" type="HistoryPrescriberChoice" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="HistorySource" type="HistorySourceWithoutPayment" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ReasonForSubstitutionCodeUsed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitScript" type="SplitScriptIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Two prescriptions for the same product that are sent to two different entities for fulfillment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManufacturerName" type="an1..105" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="LotNumber" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="RxFillIndicator" type="RxFillIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the status types the sender would like to receive for this patient/product.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DiabeticSupply" type="DiabeticSupplyType" minOccurs="0"/>
			<xsd:element name="InjuryRelated" type="InjuryRelated" minOccurs="0"/>
			<xsd:element name="Service" type="ServiceStructure" minOccurs="0"/>
			<xsd:element name="IVAdministration" type="IVAdministrationType" minOccurs="0"/>
			<xsd:element name="TreatmentIndicator" type="TreatmentIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProphylacticOrEpisodic" type="ProphylacticOrEpisodic" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrentTreatmentCycle" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfCyclesPlanned" type="n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PatientCodifiedNote" type="PatientCodifiedNote" minOccurs="0" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>Provides information to the dispensing pharmacy which clarifies/justifies the product selection, dispensing, or instructs the dispensing pharmacy to take specific action per request of the provider.   It codifies items commonly found in Note; and is specific to the patient and the medication/product being prescribed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FacilitySpecificHoursOfAdministrationTiming" type="FacilitySpecificHoursOfAdministrationTiming" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Provides information to the dispensing pharmacy which clarifies/justifies the product selection, dispensing, or instructs the dispensing pharmacy to take specific action per request of the provider.   It codifies items commonly found in the element Note; and is specific to the patient and the medication/product being prescribed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherMedicationDates" type="OtherMedicationDate" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Applicable Date or Date and Time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedNoLaterThan" type="NeedNoLaterThan" minOccurs="0"/>
			<xsd:element name="PlaceOfServiceNonSelfAdministeredProduct" type="n2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code identifying the place where the non-self-administered product will be administered for the ambulatory patient.  Required when product prescribed is not to be self-administered and patient is in an ambulatory setting. The CMS Place of Service code set as the ECL values https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set.html</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProcedureCodes" type="QualifiedCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used to transmit procedure codes, such as CDT and CPT</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TypeOfDose" type="TypeOfDoseCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code representing the type of dose being requested.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MedicationAbstract  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MedicationAbstract">
		<xsd:annotation>
			<xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.
</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="an1..105">
				<xsd:annotation>
					<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If Compound, this field must contain the prescriber’s preferred name of the compound.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Product" type="ProductCompound" minOccurs="0"/>
			<xsd:element name="Quantity" type="QuantityAbstract" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DaysSupply" type="n1..3" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PADaysSupplyDosePerDay" type="PADaysSupplyDosesPerDayChoice" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="WrittenDate" type="DateType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="LastFillDate" type="DateType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Substitutions" type="SubstitutionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code indicating the prescriber's instructions regarding generic or interchangeable biosimilar substitution were followed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The refill quantity of the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="Diagnosis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="PriorAuthorizationNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the payer-assigned authorization number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..210" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DrugUseEvaluation" type="DrugUseEvaluation" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="PriorAuthorizationStatus" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sig" type="Sig" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PharmacyRequestedRefills" type="n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Number of refills the pharmacy is requesting.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="DoNotFill" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RefillsRemaining" type="n1..5" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The number of refills remaining in the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPrescriberOrderNumber" type="an1..35" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="OrderCaptureMethod" type="OrderCaptureMethod" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="OptionalPharmacy" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Prescriber" type="HistoryPrescriberChoice" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="HistorySource" type="HistorySourceAbstract" minOccurs="0" maxOccurs="unbounded"/>
