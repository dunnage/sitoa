	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AdministrationTimingUpdate  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AdministrationTimingUpdate">
		<xsd:annotation>
			<xsd:documentation>Used to updated facility specific hours of administration.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EffectiveDate" type="UtcDateType">
				<xsd:annotation>
					<xsd:documentation>Effective date of the administration timing update.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ApprovedAbstractType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ApprovedAbstractType">
		<xsd:sequence>
			<xsd:element name="ReasonCode" type="ReasonCode" minOccurs="0" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>Code used to clarify the content of a message.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="an1..70" minOccurs="0"/>
			<xsd:element name="PriorDispensing" type="PriorDispensingType" minOccurs="0"/>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ApprovedWithoutReasonType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ApprovedWithoutReasonType">
		<xsd:complexContent>
			<xsd:restriction base="ApprovedAbstractType">
				<xsd:sequence>
					<xsd:element name="ReasonCode" type="ReasonCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Code used to clarify the content of a message.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Note" type="an1..70" minOccurs="0"/>
					<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:restriction>
