			<xsd:element name="SocialSecurity" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The 9-digit number assigned to an individual by the Social Security Administration for various purposes, including paying and reporting taxes.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NPI" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A National Provider System (NPS) assigned identifier for health care providers.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CertificateToPrescribe" type="an1..35" minOccurs="0">
