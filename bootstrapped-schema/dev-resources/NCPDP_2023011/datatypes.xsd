	<!-- =====  Complex Type Definitions  -->
	<!-- ================================================== -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AddressType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AddressType">
		<xsd:sequence>
			<xsd:element name="AddressLine1" type="an1..40" minOccurs="0"/>
			<xsd:element name="AddressLine2" type="an1..40" minOccurs="0"/>
			<xsd:element name="City" type="an1..35" minOccurs="0"/>
			<xsd:element name="StateProvince" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See ECL for additional information. State must be deliverable in the country (e.g., when exchanging US States, the two-digit alpha code is to be used.  (ISO-3166-2 link from http://www.iso.org/iso/country_codes)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PostalCode" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Postal Code Deliverable formats. For example: United States format is 5 or 9 numeric digits. See ECL for additional information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CountryCode" type="an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Required if country is not “US”. Add CountryCode when it is in an optional address type. See ECL for additional information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Extension" type="Extension" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  BinaryDataType  <<complexType>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="BinaryDataType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="LengthBytes" type="xsd:int" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
